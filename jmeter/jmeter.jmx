        <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Count PostProcessor" enabled="false">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// 스레드 세이프한 방식으로 처리
synchronized(this) {
    // user 카운트
    def userId = vars.get(&quot;userId&quot;)
    def key = &quot;userId_&quot; + userId

    def userCount = props.getProperty(key, &quot;0&quot;).toInteger()
    userCount += 1
    props.put(key, userCount.toString())

    // reservableTime 카운트
    def reservableTimeId = vars.get(&quot;reservableTimeId&quot;)
    def requestQuantity = vars.get(&quot;requestQuantity&quot;).toInteger()

    def timeKey = &quot;reservableTimeId_&quot; + reservableTimeId
    def timeCount = props.getProperty(timeKey, &quot;0&quot;).toInteger()
    timeCount += requestQuantity
    props.put(timeKey, timeCount.toString())
}</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223PostProcessor>
